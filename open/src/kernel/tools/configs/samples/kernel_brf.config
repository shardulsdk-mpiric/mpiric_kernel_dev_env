CONFIG_BPF_SYSCALL=y
CONFIG_BPF_JIT=y
CONFIG_BPF_LSM=y
CONFIG_DEBUG_INFO_BTF=y
#Dependency for CONFIG_MODULE_ALLOW_BTF_MISMATCH
CONFIG_DEBUG_INFO_BTF=y
CONFIG_PAHOLE_HAS_SPLIT_BTF=y
CONFIG_MODULE_ALLOW_BTF_MISMATCH=y
CONFIG_TEST_BPF=y
CONFIG_CGROUP_BPF=y
CONFIG_NET_ACT_BPF=y
CONFIG_NET_CLS_BPF=y
CONFIG_BPF_STREAM_PARSER=y
CONFIG_LWTUNNEL_BPF=y
CONFIG_IPV6_SEG6_BPF=y
CONFIG_LIRC=y
CONFIG_BPF_LIRC_MODE2=y
#For error:
#libbpf: failed to find '.BTF' ELF section in /host_out/linux_net_next/vmlinux
#Error: failed to load BTF from /host_out/linux_net_next/vmlinux: No data available
CONFIG_DEBUG_INFO_BTF=y
#Kernel Address Sanitizer
HAVE_KCSAN_COMPILER=y
CONFIG_KCSAN=y
